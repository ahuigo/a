{
  "title": "Ctrl+u/w to arrow del",
  "rules": [
    {
      "description": "Ctrl+u/w to (niz)...",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [
                "control"
              ]
            }
          },
          "to": {
              "key_code": "delete_or_backspace",
              "modifiers": [
                "option"
              ]
            }
        },{
          "type": "basic",
          "from": {
            "key_code": "delete_or_backspace",
            "modifiers": { "mandatory": [ "option" ] }
          },
          "to": {
              "key_code": "w",
              "modifiers": [ "control" ]
          }
        },{
          "type": "basic",
          "from": {
            "key_code": "u",
            "modifiers": { "mandatory": [ "control" ] }
          },
          "to": {
              "key_code": "delete_or_backspace",
              "modifiers": [
                "command"
              ]
            }
        },{
          "type": "basic",
          "from": { "key_code": "f10", "modifiers": { "mandatory": [ "left_control" ] }
          },
          "to": {
              "key_code": "volume_decrement"
           }
        }
      ]
    }
  ]
}
